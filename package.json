{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:5000",
  "scripts": {
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js",
    "build-css": "node-sass-chokidar src/ --include-path ./src -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ --include-path ./src -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "format": "prettier \"src/**/*.{js,json,css}\" --write"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "jsxBracketSameLine": true,
    "bracketSpacing": false
  },
  "globalDependencies": {
    "react-scripts": "1.1.4"
  },
  "dependencies": {
    "axios": "0.18.0",
    "bootstrap": "3.3.7",
    "classnames": "2.2.5",
    "date-fns": "1.29.0",
    "flatpickr": "4.5.0",
    "font-awesome": "4.7.0",
    "jquery": "3.3.1",
    "js-cookie": "2.2.0",
    "lodash": "4.17.10",
    "prop-types": "15.6.1",
    "react": "16.3.2",
    "react-autobind": "1.0.6",
    "react-bootstrap": "0.32.1",
    "react-dom": "16.3.2",
    "react-flatpickr": "3.6.4",
    "react-helmet": "5.2.0",
    "react-redux": "5.0.7",
    "react-router-bootstrap": "0.24.4",
    "react-router-dom": "4.2.2",
    "redux": "4.0.0",
    "redux-thunk": "2.2.0",
    "toastr": "2.1.4"
  },
  "devDependencies": {
    "node-sass-chokidar": "1.3.0",
    "npm-run-all": "4.1.3"
  }
}
